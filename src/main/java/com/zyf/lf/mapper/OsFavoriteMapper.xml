    <!--通用查询结果列-->
    <sql id="Base_Column_List">
        favorite_id AS favoriteId, user_id AS userId, product_id AS productId, product_number AS productNumber, name, pic_img AS picImg, show_price AS showPrice, status, create_time AS createTime, create_by AS createBy
    </sql>
</mapper>
