		<result column="create_by" property="createBy" />
	</resultMap>

    <!--通用查询结果列-->
    <sql id="Base_Column_List">
        pic_img_id AS picImgId, product_id AS productId, pic_img AS picImg, sort, status, create_time AS createTime, create_by AS createBy
    </sql>
</mapper>
