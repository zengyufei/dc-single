<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zyf.lf.mapper.OsProductImageMapper">
	<!--开启二级缓存-->


	<resultMap id="BaseResultMap" type="com.zyf.lf.models.entity.OsProductImage">
		<id column="pic_img_id" property="picImgId" />
		<result column="product_id" property="productId" />
		<result column="pic_img" property="picImg" />
		<result column="sort" property="sort" />
		<result column="status" property="status" />
